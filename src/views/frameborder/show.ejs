<line class="frameborder-line" x2="<%=width%>" /> <!-- top: start in x: 0 and go to x: 560 -->
<line class="frameborder-line" x1="<%=width%>" x2="<%=width%>" y2="<%= height %>" /> <!-- right: x forever in: MAX_WIDTH  start y:TOP and go to BOTTOM -->
<line class="frameborder-line" x1="<%=width%>" y1="<%= height %>" y2="<%= height %>"/> <!-- bottom: x: MAX_WIDTH to 0 and Y in BOTTOM -->
<line class="frameborder-line" y2="<%= height %>"/> <!-- left: start all in 0 and go to down  -->